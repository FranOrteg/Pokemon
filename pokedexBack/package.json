{
  "name": "pokedexback",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "dev": "nodemon ./bin/www",
    "database-dump": "mysqldump -u root  -p Pokedex > dump.sql",
    "database-restore": "cat ./dump.sql | docker exec -i mysql  /usr/bin/mysql -u root --password=password",
    "database-start": "docker run --name mysql -p 3307:3306 --rm -e MYSQL_ROOT_PASSWORD=password -d mysql:latest"
  },
  "dependencies": {
    "cookie-parser": "~1.4.4",
    "cors": "^2.8.5",
    "debug": "~2.6.9",
    "dotenv": "^16.3.0",
    "express": "~4.16.1",
    "morgan": "~1.9.1",
    "mysql2": "^3.3.5",
    "nodemon": "^2.0.22"
  }
}